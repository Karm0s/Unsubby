{
  "manifest_version": 3,
  "name": "Unsubby",
  "version": "1.0",
  "description": "Remove youtube subscripitons with one click.",
  "icons": {
    "16": "ICON16.png",
    "48": "ICON48.png",
    "128": "ICON128.png"
  },
  "action": {
    "default_icon": {
      "16": "ICON16.png",
      "48": "ICON48.png",
      "128": "ICON128.png"
    },
    "default_title": "Use unsubby"
  },
  "background": {
    "service_worker": "backgroundScript.js"
  },
  "permissions": ["activeTab", "scripting"],
  "host_permissions": [
    "*://*.youtube.com/feed/channels*"
  ]
}
