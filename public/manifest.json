{
  "manifest_version": 3,
  "name": "Unsubby",
  "version": "1.0",
  "description": "Remove youtube subscripitons with one click.",
  "permissions": ["tabs"],
  "action": {
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/feed/channels*"],
      "js": ["contentScript.js"]
    }
  ],
  "action": {
    "default_popup": "index.html"
  }
}
